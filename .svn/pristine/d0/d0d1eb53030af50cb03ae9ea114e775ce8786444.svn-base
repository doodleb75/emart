            <script>
                function loadComponent(url, placeholderId) {
                    fetch(url + '?v=' + new Date().getTime())
                        .then(response => {
                            if (!response.ok) throw new Error('Network response was not ok');
                            return response.text();
                        })
                        .then(data => {
                            // Adjust relative paths for pages in subfolders
                            if (window.location.pathname.includes('/pages/')) {
                                data = data.replace(/src="assets\//g, 'src="../assets/');
                            }
                            document.getElementById(placeholderId).innerHTML = data;
                            if (placeholderId === 'header-placeholder') {
                                document.dispatchEvent(new Event('headerLoaded'));
                            }
                            if (placeholderId === 'footer-placeholder') {
                                document.dispatchEvent(new Event('footerLoaded'));
                            }
                        })
                        .catch(error => console.error('Error loading component:', error));
                }

                loadComponent('/components/header.component.txt', 'header-placeholder');
                loadComponent('/components/footer.component.txt', 'footer-placeholder');
            </script>

